{
  "include": [
    "."
  ],
  "exclude": [
    "./server",
    "./node_modules",
  ],
  "compilerOptions": {
    "jsx": "react",
    "checkJs": true,
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "bundler",
    "moduleDetection": "force",
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "composite": true,
    "strict": true,
    "downlevelIteration": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": true
  },
  "extends": "expo/tsconfig.base"
}
